{
  "name": "ritm-site-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev-server": "ts-node -P tsconfig.server.json server/index.ts",
    "build-server": "tsc -p tsconfig.server.json",
    "start-server": "node dist-server/index.js",
    "lint": "next lint",
    "format": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "check": "npm run lint && npm run format",
    "prepare": "husky"
  },
  "dependencies": {
